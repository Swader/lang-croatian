{
    "name": "bryantmilan/lang-serbian",
    "description": "Serbian language pack for Flarum.",
    "type": "flarum-extension",
    "keywords": ["extension", "flarum", "serbian", "locale", "translation"],
    "license": "MIT",
    "authors": [
        {
            "name": "Milan Bryant",
            "email": "milansamp999@gmail.com"
        }
    ],
    "support": {
        "issues": "https://github.com/bryantmilan/lang-serbian/issues",
        "source": "https://github.com/bryantmilan/lang-serbian"
    },
    "require": {
        "flarum/core": "^0.1.0-beta.13"
    },
    "extra": {
        "branch-alias": {
            "dev-master": "1.x-dev"
        },
        "flarum-extension": {
            "title": "Serbian",
            "icon": {
                "image": "icon.svg",
                "backgroundColor": "#fff",
                "backgroundSize": "cover",
                "backgroundPosition": "center"
            }
        },
        "flarum-locale": {
            "code": "sr",
            "title": "Srpski"
        }
    }
}
