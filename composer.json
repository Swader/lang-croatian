    "type": "flarum-extension",
    "keywords": ["extension", "flarum", "croatian", "locale", "translation"],
    "license": "MIT",
    "authors": [
        {
            "name": "Milan Bryant",
            "email": "milansamp999@gmail.com"
        },
        {
            "name": "Bruno Å kvorc",
